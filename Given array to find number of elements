
class Reverser1
{
    public Reverser1()
    {
        
        int[] array = {1,2,3,4,5};
        int elements = 0 ;
        for(int i = 0;i<array.length;i++)
        {
            if(array[i]>=0||array[i]<0)
            { 
                elements=elements+1;
                continue;
            }
            
            
            
        }
        System.out.println("The  number of elements in the array is"  +elements);
    }
}
